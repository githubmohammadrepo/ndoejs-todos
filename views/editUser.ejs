<%- include('./layout/header'); -%>
    <div class="container-fluid">

        <%- include('./layout/subHeader') -%>

            <div class="row justify-content-center my-4">
                <div class="col-12 m-auto  text-center col-sm-10 col-md-8 col-lg-6 col-xl-4" style="min-width:23rem !important">

                    <div class="card mt-2 bg-primary m-auto">
                        <div class="card-header">
                            <h2 class="text-light text-center">Create New User</h2>
                        </div>
                        <div class="card-body text-fwhite">
                            <form action="/users/<%= user.id %>/update" method="POST">

                                <!-- row for first name and last name input -->
                                <div class="row">
                                    <!-- first name input group -->
                                    <div class="col form-group">
                                        <label for="firstName">first Name</label>
                                        <input type="text" class="form-control" name="firstName" id="firstName" value="<%= user.firstName %>" placeholder="john...">
                                    </div>

                                    <!-- last name input group -->
                                    <div class="col form-group">
                                        <label for="lastName">last Name</label>
                                        <input type="text" class="form-control" name="lastName" id="lastName" value="<%= user.lastName %>" placeholder="doe ...">
                                    </div>
                                </div>

                                <!-- email input group -->
                                <div class="form-group">
                                    <label for="Email">Email Address</label>
                                    <input type="email" class="form-control" name="email" id="Email" value="<%= user.email %>" placeholder="example@gamil.com">
                                </div>

                                <!-- image url input group -->
                                <div class="form-group">
                                    <label for="imageUrl">Image Url</label>
                                    <input type="text" class="form-control" id="imageUrl" name="imageUrl" value="<%= user.Images[0].src %>" placeholder="data:image/png;base64,">
                                </div>

                                <!-- iamge title input group -->
                                <div class="form-group">
                                    <label for="imageTitle">Image title</label>
                                    <input type="text" class="form-control" name="imageTitle" id="imageTitle" value="<%= user.Images[0].title %>" placeholder="image title">
                                </div>

                                <!-- <div class="form-group">
              <label for="exampleFormControlSelect1">Example select</label>
              <select class="form-control" id="exampleFormControlSelect1">
                <option>1</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
              </select>
            </div> -->
                                <div class="form-group text-center mb-0">
                                    <button type="submit" class="btn btn-navy text-fwhite">Update User</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

    </div>
    <%- include('./layout/footer'); -%>